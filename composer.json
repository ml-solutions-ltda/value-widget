{
    "name": "mlsolutions/value-widget",
    "description": "A basic widget for laravel nova-dashboard.",
    "keywords": [
        "laravel",
        "nova",
        "nova-dashboard",
        "widget"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        },
        {
            "name": "ML Solution",
            "email": "ti@mlsolutions.net.br"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "MlSolutions\\ValueWidget\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MlSolutions\\ValueWidget\\ValueWidgetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
